{
    "build": {
        "beforeDevCommand": "",
        "beforeBuildCommand": "",
        "devPath": "../src",
        "distDir": "../src",
        "withGlobalTauri": true
    },
    "package": {
        "productName": "prac",
        "version": "0.0.0"
    },
    "tauri": {
        "systemTray": {
            "iconPath": "icons/icon.png",
            "iconAsTemplate": true
        },
        "allowlist": {
            "all": false,
            "clipboard": {
                "all": true,
                "writeText": true,
                "readText": false
            }
        },
        "bundle": {
            "active": true,
            "icon": [
                "icons/32x32.png",
                "icons/128x128.png",
                "icons/128x128@2x.png",
                "icons/icon.icns",
                "icons/icon.ico"
            ],
            "identifier": "fluffydolpihn",
            "targets": "all"
        },
        "security": {
            "csp": null
        },
        "updater": {
            "active": false
        },
        "windows": [{
            "fullscreen": false,
            "resizable": true,
            "title": "prac",
            "width": 550,
            "height": 600
        }]
    }
}